<%% human_name = <%= class_name %>.model_name.human) %>
<%% @<%= singular_table_name %> = <%= class_name %>Presenter.new( row, self ) %>
<tr id="<%= singular_table_name %>_<%%= row.id %>">
<% editable_attributes.map.each do |a| -%>
  <td><%%= @<%= singular_table_name %>.<%= a.name %> %>
<% end -%>
<td class="links">
  <%%= link_to  icon('edit', tooltip: t(:edit, model: human_name)), <%= edit_helper.gsub(/_url/,"_path") %>, data: { toggle: 'modal', target: '#Modal' } %>
  <%%= link_to icon('cancel', tooltip: t(:delete, model: human_name)), <%= show_helper.gsub(/_url/,"_path") %>, remote: true, method: :delete, data: { confirm: t(:confirm_delete_record, model: human_name) } %>


